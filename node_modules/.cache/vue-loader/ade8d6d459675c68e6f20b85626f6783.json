kbox:checked + .onoffswitch-label .onoffswitch-switch {\n        right: 0px;\n        background-color: #27A1CA;\n    }\n\n    .onoffswitch label {\n        float: left;\n        font-size: 23px;\n    }\n\n    .onoffswitch div {\n        float: left;\n        font-size: 23px;\n    }\n\n    .father {\n        /*background: #71b5e5;*/\n        width: 100%;\n        margin: 5px 0px 0px 18px;\n        position: relative;\n    }\n\n    .father label {\n        width: 200px;\n        float: left;\n        font-size: 15px;\n        font-weight: bold;\n    }\n\n    .father form {\n        float: left;\n        font-size: 23px;\n    }\n\n    .word {\n        /*position: relative;*/\n        margin: 10px 0px 0px 0px;\n    }\n\n    .word label {\n        font-weight: bold;\n        font-size: 20px;\n    }\n\n    .hrLine {\n        vertical-align: middle;\n        display: inline-block;\n        width: 80%;\n    }\n    .rightNav {\n        width: 100%;\n        height: 100%;\n        display: inline-flex;\n        .tab-nav {\n            overflow: hidden;\n            border-right: 1px solid #c2c2c2;\n            list-style: none;\n            width: 250px;\n            height: auto;\n        }\n    }\n    //点击后右箭头的反转效果\n    .nav-n-box-active {\n        #right-btn {\n            transform: rotate(90deg) !important;\n        }\n    }\n</style>\n<style scoped>\n    .layout{\n        border: 1px solid #d7dde4;\n        background: #f5f7f9;\n        position: relative;\n        border-radius: 4px;\n        overflow: hidden;\n    }\n    .layout-logo{\n        /*width: 100px;*/\n        height: 30px;\n        /*background: #5b6270;*/\n        border-radius: 3px;\n        float: left;\n        position: relative;\n        /*top: 15px;*/\n        left: 20px;\n    }\n    .layout-nav{\n        width: 720px;\n        margin: 0 auto;\n        margin-right: 20px;\n    }\n</style>\n"]}]}